{
  "name": "geomania",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run export:countries-json && npm run generate:pwa-icons",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview",
    "fetch:flags": "node --import tsx scripts/fetch-flags.ts",
