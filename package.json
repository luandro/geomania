    "update:countries": "node --import tsx scripts/update-countries-data.ts",
    "verify:flag-conflicts": "node --import tsx scripts/generate-flag-conflicts.ts --check",
    "verify:public-flags": "node --import tsx scripts/verify-flags-dir.ts --dir public/flags",
    "verify:dist-flags": "node --import tsx scripts/verify-flags-dir.ts --dir dist/flags",
    "export:countries-json": "node --import tsx scripts/export-countries-json.ts",
    "generate:pwa-icons": "node --import tsx scripts/generate-pwa-icons.ts",
    "prepare": "husky"
